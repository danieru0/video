  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dropbox": "^4.0.9",
    "es6-promise": "^4.2.4",
    "express": "^4.16.2",
    "express-validator": "^5.3.0",
    "firebase": "^5.3.1",
    "isomorphic-fetch": "^2.2.1"
  }
}
